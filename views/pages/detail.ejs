<!DOCTYPE html>
<html lang='en'>

<head>
    <% include ../partials/detail %>
        <meta name="viewport" content="width=device-width, initial-scale=1">
</head>

<body class='container'>

    <header>
        <% include ../partials/header %>
    </header>
    <main>
        <div class="main-photo-info">
            <img src='<%= detail.link %>' class='main-photo'>
            <br>
            <%= detail.class %>
                <br>
                <%= detail.desc %>
        </div>


        <div class='well'>
            <% photos.forEach(function(photos) { %>

                <img src=' <%= photos.link %>' class='well-image'>

                <% }); %>
        </div>
        <form action='/gallery/<%- photo.photo_id %>/edit' method="put">
            <input type="submit" value="Edit" />
        </form>
        <form action='/gallery/<%- photo.photo_id %>' method="delete">
            <input type="submit" value="Delete" />
        </form>
    </main>
    <footer>
        <% include ../partials/footer %>
    </footer>
</body>

</html>